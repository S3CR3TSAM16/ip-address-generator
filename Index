<script>
function generateIPAddresses() {
    var ipAddressList = "";
    for (var i = 0; i < 1000; i++) { // Generating 1000 IP addresses
        var ip = [];
        for (var j = 0; j < 4; j++) {
            ip.push(Math.floor(Math.random() * 256));
        }
        ipAddressList += ip.join(".") + " 
        ";
    }
    document.getElementById("ipAddresses").innerHTML = ipAddressList;
}

function copyIPAddresses() {
    var ipAddresses = document.getElementById("ipAddresses").innerHTML;
    navigator.clipboard.writeText(ipAddresses);
    alert("IP addresses copied to clipboard!");
}
</script>
</head>
<body>
<div id="container">
    <h1>Bulk IP Address Generator</h1>
    <h2>By: SECRET </h2>
    <button onclick="generateIPAddresses()">IP Addresses Generate Kare</button>
    <button onclick="copyIPAddresses()">IP Addresses Copy Kare</button>
    <div id="ipAddresses"></div>
</div>
